@page
@model PaperWorks.ConfirmCasemModel
@{
    ViewData["Title"] = "CaseConfirm";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<section id="caseConfirmationSection">
    <h4 class="display-4 m-3 text-muted text-center" data-aos="fade-left" data-aos-delay="800">@Model.MessageForConsultant</h4>
    <a class="text-center" asp-page="/Consultant/MyCases" >Click here to See Your Cases</a>
</section>


@section Scripts{

    <script type="text/javascript">
        $(document).ready(function () {
            $("#caseConfirmationSection").css("margin-top", $("#topnavbarbutnobeer").height() * 1.3);
        });
    </script>
}

